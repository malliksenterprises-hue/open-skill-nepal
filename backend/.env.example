# Server Configuration
NODE_ENV=development
PORT=8080
FRONTEND_URL=https://openskillnepal.com
BACKEND_URL=https://your-backend-url.asia-south1.run.app

# MongoDB
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/openskillnepal

# JWT
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRES_IN=24h

# Google Cloud Storage
GCS_BUCKET_NAME=openskillnepal-videos
GOOGLE_APPLICATION_CREDENTIALS=path-to-service-account-key.json

# Email Service (Future Use)
EMAIL_SERVICE=gmail
EMAIL_USER=your-email@gmail.com
EMAIL_PASS=your-app-password

# Admin Credentials (Initial Setup)
SUPER_ADMIN_EMAIL=superadmin@example.com
SUPER_ADMIN_PASSWORD=password
