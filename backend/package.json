{
  "name": "open-skill-nepal-backend",
  "version": "2.0.0",
  "description": "Open Skill Nepal Backend - Phase 2 Complete",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
