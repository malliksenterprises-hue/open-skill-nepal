{
  "name": "open-skill-nepal-backend",
  "version": "3.1.0",
  "description": "Open Skill Nepal Backend - Professional Grade API with Live Classes & Device Limits",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:device": "jest backend/tests/DeviceService.test.js --coverage",
    "test:integration": "jest backend/tests/integration/*.test.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "security-audit": "npm audit --audit-level=moderate",
    "deploy:production": "gcloud run deploy open-skill-nepal --source . --region asia-south1",
    "seed": "node scripts/seed.js",
    "seed:test": "NODE_ENV=test node scripts/seed.js",
    "migrate:devices": "node scripts/migrate-device-fields.js",
    "migrate:schools": "node scripts/migrate-school-device-limits.js",
    "cleanup:devices": "node scripts/cleanup-inactive-devices.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "winston": "^3.11.0",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "@google-cloud/storage": "^6.9.5",
    "fluent-ffmpeg": "^2.1.2",
    "ffmpeg-static": "^5.1.0",
    "node-cron": "^3.0.3",
    "axios": "^1.6.0",
    "moment": "^2.29.4",
    "joi": "^17.11.0",
    "socket.io": "^4.7.2",
    "mediasoup": "^3.11.12",
    "mediasoup-client": "^3.6.63",
    "uuid": "^9.0.1",
    "prom-client": "^14.2.0",
    "@fingerprintjs/fingerprintjs": "^4.2.1",
    "clientjs": "^0.2.1",
    "device-uuid": "^1.0.5",
    "crypto-js": "^4.2.0",
    "user-agents": "^1.1.159",
    "geoip-lite": "^1.4.7",
    "express-useragent": "^1.0.15",
    "http-status-codes": "^2.3.0",
    "socket.io-redis": "^6.1.1",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "eslint": "^8.57.0",
    "supertest": "^6.3.3",
    "mongodb-memory-server": "^9.1.1",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/mongoose": "^5.11.97",
    "@types/supertest": "^6.0.2",
    "ts-jest": "^29.1.1",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "edtech",
    "nepal",
    "education",
    "api",
    "mongodb",
    "webrtc",
    "livestream",
    "socket.io",
    "device-limits",
    "security"
  ],
  "author": "Open Skill Nepal Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/openskillnepal/open-skill-nepal.git",
    "directory": "backend"
  },
  "bugs": {
    "url": "https://github.com/openskillnepal/open-skill-nepal/issues"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/",
      "/scripts/"
    ],
    "collectCoverageFrom": [
      "services/**/*.js",
      "controllers/**/*.js",
      "models/**/*.js",
      "middleware/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
