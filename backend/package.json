{
  "name": "open-skill-nepal-backend",
  "version": "2.0.0",
  "description": "Open Skill Nepal Backend - Professional Grade API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
