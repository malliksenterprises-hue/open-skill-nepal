{
  "name": "open-skill-nepal-backend",
  "version": "2.0.0",
  "description": "Open Skill Nepal Phase 2 - Professional Backend with GCP Integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:debug": "nodemon --inspect server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config ./test/jest.e2e.config.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "security:audit": "npm audit",
    "security:outdated": "npm outdated",
    "build:docker": "docker build -t open-skill-nepal:phase2 .",
    "deploy:gcp": "gcloud run deploy open-skill-nepal --image gcr.io/open-skill-nepal-478611/open-skill-nepal:phase2",
    "migrate:up": "node scripts/migrations/up.js",
    "migrate:down": "node scripts/migrations/down.js",
    "seed": "node scripts/seed.js",
    "setup:gcp": "node scripts/setup-gcp.js",
    "health:check": "curl http://localhost:8080/health/detailed",
    "logs": "docker-compose logs -f",
    "docs:generate": "apidoc -i src/ -o docs/",
    "clean": "rm -rf node_modules && npm cache clean --force",
    "precommit": "npm run lint && npm test",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "education",
    "nepal",
    "video-platform",
    "gcp",
    "mongodb",
    "express"
  ],
  "author": "Open Skill Nepal Team",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/storage": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "validator": "^13.11.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "apidoc": "^1.2.0"
  }
}
