{
  "name": "open-skill-nepal-frontend",
  "version": "1.1.0",
  "description": "Frontend for Open Skill Nepal EdTech Platform with Live Classes & Device Management",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:device": "jest __tests__/DeviceValidationService.test.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "next": "14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.0.1",
    "postcss": "^8.4.0",
    "@fingerprintjs/fingerprintjs": "^4.2.1",
    "socket.io-client": "^4.7.2",
    "mediasoup-client": "^3.6.63",
    "uuid": "^9.0.1",
    "react-error-boundary": "^4.0.11",
    "@mui/material": "^5.14.18",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.18",
    "axios": "^1.6.2",
    "react-hook-form": "^7.48.2",
    "zod": "^3.22.4",
    "@hookform/resolvers": "^3.3.2",
    "react-hot-toast": "^2.4.1",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "react-query": "^3.39.3",
    "react-icons": "^4.12.0",
    "react-intersection-observer": "^9.5.3",
    "react-virtualized-auto-sizer": "^1.0.23",
    "react-window": "^1.8.10",
    "recharts": "^2.10.3",
    "jwt-decode": "^4.0.0",
    "qs": "^6.11.2",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dropzone": "^14.2.3",
    "react-helmet-async": "^2.0.4",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^14.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-config-next": "14.0.0",
    "@types/react": "^18.2.0",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.3.0",
    "jest": "^29.6.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "jest-environment-jsdom": "^29.6.2",
    "@storybook/react": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/test": "^7.5.3",
    "storybook": "^7.5.3",
    "prettier": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint-plugin-storybook": "^0.6.15"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "edtech",
    "nepal",
    "education",
    "nextjs",
    "react",
    "live-classes",
    "webrtc",
    "device-management",
    "tailwindcss"
  ],
  "author": "Open Skill Nepal Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/openskillnepal/open-skill-nepal.git",
    "directory": "frontend"
  },
  "bugs": {
    "url": "https://github.com/openskillnepal/open-skill-nepal/issues"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "components/**/*.{js,jsx,ts,tsx}",
      "services/**/*.{js,jsx,ts,tsx}",
      "utils/**/*.{js,jsx,ts,tsx}",
      "app/**/*.{js,jsx,ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/.next/",
      "/out/",
      "/storybook-static/"
    ]
  }
}
